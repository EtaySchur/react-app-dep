{
  "name": "dependency-upgrade-analyzer",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development --open",
    "start:no-open": "webpack serve --mode development",
    "build": "webpack --mode production",
    "server": "node server/start.js",
    "server:dev": "nodemon server/start.js",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "analyze-imports": "ts-node src/analyzeImports.ts",
    "find-package-usage": "ts-node src/findPackageUsage.ts",
    "find-symbol-usage": "ts-node src/findSymbolUsage.ts",
    "analyze-symbol-ast": "ts-node src/analyzeSymbolAst.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EtaySchur/ts-diff.git"
  },
  "keywords": [
    "dependency-analysis",
    "breaking-changes",
    "migration",
    "upgrade-tool",
    "api-deprecation",
    "version-compatibility"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EtaySchur/ts-diff/issues"
  },
  "homepage": "https://github.com/EtaySchur/ts-diff#readme",
  "description": "A comprehensive tool for analyzing dependency upgrades and detecting breaking changes in JavaScript/TypeScript projects. Features real-world examples of deprecated APIs and migration patterns.",
  "dependencies": {
    "ag-grid-react": "28.2.1",
    "axios": "1.6.4",
    "chalk": "^4.1.0",
    "class-validator": "^0.13.2",
    "create-react-context": "^0.3.0",
    "date-fns-tz": "^2.0.0",
    "express": "3.21.2",
    "formik": "1.5.8",
    "highlight.js": "9.16.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-hook-form": "6.15.8",
    "react-query": "^3.39.3",
    "react-router-dom": "4.3.1",
    "web-vitals": "^2.1.4",
    "zod": "3.17.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@playwright/test": "^1.53.1",
    "@types/highlight.js": "^9.12.4",
    "@types/react": "^16.14.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.1",
    "babel-loader": "^10.0.0",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "karma": "5.0.0",
    "nodemon": "^3.0.2",
    "playwright": "^1.53.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.3",
    "webpack": "^5.99.5",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  }
}
